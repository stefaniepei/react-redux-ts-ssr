{
  "name": "react-redux-ts-ssr",
  "version": "1.0.5",
  "description": "react16+react-router4+redux+ts",
  "pre-commit": [
    "lint"
  ],
  "scripts": {
    "start": "better-npm-run start",
    "start:server": "better-npm-run start:server",
    "clean": "rimraf dist",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:qa": "better-npm-run deploy:qa",
    "start:dist": "better-npm-run start:dist",
    "server": "better-npm-run server",
    "dev": "better-npm-run dev",
    "qa": "better-npm-run qa"
  },
  "betterScripts": {
    "start": {
      "command": "ts-node build/client/index.ts",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "start:dist": {
      "command": "node dist/server.bundle.js",
      "env": {
        "NODE_ENV": "production",
        "PORT": "9012",
        "RENDER_TYPE": "server",
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run clean && ts-node build/server/compile.ts",
      "env": {
        "NODE_ENV": "development",
        "PORT": "9010",
        "RENDER_TYPE": "server",
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "npm run deploy:dev && npm run start:dist",
      "env": {
        "NODE_ENV": "development",
        "PORT": "9010",
        "RENDER_TYPE": "server",
        "DEBUG": "app:*"
      }
    },
    "deploy:qa": {
      "command": "npm run clean && ts-node build/server/compile.ts",
      "env": {
        "NODE_ENV": "qa",
        "PORT": "9011",
        "RENDER_TYPE": "server",
        "DEBUG": "app:*"
      }
    },
    "qa": {
      "command": "npm run deploy:qa && npm run start:dist",
      "env": {
        "NODE_ENV": "qa",
        "PORT": "9011",
        "RENDER_TYPE": "server",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && ts-node build/server/compile.ts",
      "env": {
        "NODE_ENV": "production",
        "PORT": "9012",
        "RENDER_TYPE": "server",
        "DEBUG": "app:*"
      }
    },
    "server": {
      "command": "npm run deploy && npm run start:dist",
      "env": {
        "NODE_ENV": "production",
        "PORT": "9012",
        "RENDER_TYPE": "server",
        "DEBUG": "app:*"
      }
    }
  },
  "author": "stefaniepei",
  "license": "MIT",
  "dependencies": {
    "antd": "^3.3.3",
    "axios": "^0.18.0",
